{
    "swagger": {
        "outputDirectory": "./src/swagger",
        "entryFile": "./src/controllers/serviceController.ts",
        "yaml": false,
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "securityDefinitions": {
            "api_key": {
                "type": "apiKey",
                "name": "access_token",
                "in": "query"
            }
        }
    }
}